@model Khabarho.Models.AccountModels.User

@{
    ViewBag.Title = "title";
    Layout = "_LayoutPersonalAccount";
}

<div class="mt-2 container_post">
    
<table class="table">
  <thead> 
    <tr>
      <th scope="col">E-mail</th>
      <th scope="col">Никнейм</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>@Model.Email</td>
      <td>@Model.UserName</td>
      <td>
        <input type="button" class="btn btn-danger btn-sm" value="Удалить" onclick="location.href='@Url.Action("Delete", "Account")?id=@Model.Id'">
      </td>
    </tr>
  </tbody>
</table>

</div>