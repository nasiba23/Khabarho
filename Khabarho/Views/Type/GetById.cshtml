@model Khabarho.ViewModels.TypeViewModel

@{
    ViewBag.Title = "title";
    Layout = "_LayoutAdmin";
}
<div class="mt-4 container_post">
    
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Название</th>
      <th scope="col">Дата создания</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">@Model.Id</th>
      <td>@Model.Title</td>
      <td>@Model.CreatedDate</td>
    </tr>
  </tbody>
</table>

</div>